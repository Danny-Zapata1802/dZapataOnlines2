<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="dZapataOnlines2.LoginDeber">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Login!" />
            <Entry x:Name="txtUsuario" Placeholder="ingrese su usuario"/>
            <Entry x:Name="txtPassword" IsPassword="True" Placeholder="ingrese su contraseña"/>
            <Button x:Name="btnLogin" Text="Iniciar sesión" Clicked="btnLogin_Clicked"/>
        </StackLayout>

    </ContentPage.Content>
</ContentPage>